#!/bin/bash

# set -x

ipc_server_folder="/tmp/9572cf67-b586-4c68-a7da-7cb904b396b3"

_control() {
  local prev=${COMP_WORDS[((COMP_CWORD - 1))]}

  case ${prev} in
    -s)
      cd "${ipc_server_folder}" 2>/dev/null && _filedir
      ;;
    *)
      _filedir
      ;;
  esac
}

complete -F _control control
